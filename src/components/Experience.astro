---
import ExperienceItem from "./ExperienceItem.astro";

const experience = [
  {
    date: "Actualmente En Desarrollo",
    title: "React Shop with Asp.Net Core and PostgreSQL",
    description:
      "Una pequeña aplicación de administración de productos y ventas, aun sigue en el desarrollo.",
    link: "",
  },
];
---

<ol class="relative border-s border-gray-200 ml-2">
  {
    experience.map((experience) => (
      <li class="mb-10 ms-4">
        <ExperienceItem {...experience} />
      </li>
    ))
  }
</ol>
